{
	"Doxygen File Header": {
		"prefix": "headerfile",
		"body": [
			"/*!\t\t\tCopyright(c) 2008-2017 xxxx",
			" *\\file\t\t$TM_FILENAME",
			" *\\brief\t",
			" *\\details\t",
			" *",
			" *\\author\t",
			" *\\version\t",
			" *\\date\t\t",
			" *",
			" *\\warning\t",
			" *",
			" *\\history \\arg ",
			" *         \\arg ",
			" */"
		]
	},
	"Doxygen Function Header": {
		"prefix": "headerfunc",
		"body": [
			"/*!",
			" *\\fn\t\t\t",
			" *\\brief\t\t",
			" *\\details\t\t",
			" *",
			" *\\param[in]\t",
			" *\\param[in]\t",
			" *\\param[out]\t",
			" *",
			" *\\return\t\t",
			" *\\retval\t\t",
			" *",
			" *\\note\t\t\t",
			" */"
		]
	},
	"Doxygen Struct Header": {
		"prefix": "headerstruct",
		"body": [
			"/*!",
			" *\\struct\t\t",
			" *\\brief\t",
			" */"
		]
	},
	"Doxygen Enum Header": {
		"prefix": "headerenum",
		"body": [
			"/*!",
			" *\\enum\t\t",
			" *\\brief\t",
			" */"
		]
	},
	"Doxygen Typedef Header": {
		"prefix": "headertype",
		"body": [
			"/*!",
			" *\\typedef\t",
			" *\\brief\t",
			" */"
		]
	},
	"Doxygen Var Header": {
		"prefix": "headervar",
		"body": [
			"/*!",
			" *\\var\t\t",
			" *\\brief\t",
			" */"
		]
	},
	"Doxygen Header Content": {
		"prefix": "contentheader",
		"body": [
			"/***************************************************************************/",
			"/*                           CONFIGURATIONS                                */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                           INCLUDE_FILES                                 */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                              DEFINES                                    */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                               TYPES                                     */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                              VARIABLES                                  */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                              FUNCTIONS                                  */",
			"/***************************************************************************/"
		]
	},
	"Doxygen C Content": {
		"prefix": "contentc",
		"body": [
			"/***************************************************************************/",
			"/*                           CONFIGURATIONS                                */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                            INCLUDE_FILES                                */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                               DEFINES                                   */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                               TYPES                                     */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                            EXTERN_PROTOTYPES                            */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                            LOCAL_PROTOTYPES                             */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                               VARIABLES                                 */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                            LOCAL_FUNCTIONS                              */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                            PUBLIC_FUNCTIONS                             */",
			"/***************************************************************************/",
			"",
			"",
			"",
			"/***************************************************************************/",
			"/*                            GLOBAL_FUNCTIONS                             */",
			"/***************************************************************************/"
		]
	},
	"Avoid repeated inclusion": {
		"prefix": "ifn",
		"body": "#ifndef ${1:SYMBOL}\n#define ${1:SYMBOL}\n\n#ifdef __cplusplus\nextern \"C\" {\n#endif\n\n$0\n\n#ifdef __cplusplus\n}\n#endif\n\n#endif /* ${1:SYMBOL} */"
	},
	"#ifdef #else": {
		"prefix": "ife",
		"body": "#ifdef ${1:SYMBOL}\n$0\n#else /* ${1:SYMBOL} */\n\n#endif /* ${1:SYMBOL} */"
	},
	"#ifdef": {
		"prefix": "ifd",
		"body": "#ifdef ${1:SYMBOL}\n$0\n#endif /* ${1:SYMBOL} */"
	},
	"#ifdef __cplusplus": {
		"prefix": "ccpp",
		"body": "#ifdef __cplusplus\nextern \"C\" {\n#endif\n\n$0\n\n#ifdef __cplusplus\n}\n#endif\n"
	}
}
